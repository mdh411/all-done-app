{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "stylelint": "^16.7.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint:styles": "stylelint '**/*.css' --quiet || echo 'Stylelint completed with warnings'",
    "lint": "eslint --no-ignore src/"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest",
      "^.+\\.css$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-class-properties": "^7.18.6",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-transform-numeric-separator": "^7.18.6",
    "@babel/plugin-transform-optional-chaining": "^7.18.6",
    "@babel/plugin-transform-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.0.0",
    "babel-jest": "^29.7.0",
    "cypress": "^13.13.0",
    "eslint": "^8.4.1",
    "eslint-plugin-jest": "^25.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0"
    }
}
